@model IEnumerable<AspMvcWebshop.Models.Category>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Browse categories</h3>
<p>
    Select from @Model.Count() categories: 
</p>
<ul>
    @foreach (var category in Model)
    {
        <li> 
            @Html.ActionLink(category.Name,
"Browse", new { category = category.Name })
    </li>
    }
</ul>
